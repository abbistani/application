<record id="organization" is-extension="true">
	<services-record-path id="cinefiles">organizations_cinefiles:http://collectionspace.org/services/organization/local/cinefiles,organizations_cinefiles</services-record-path>

	<instances id="organization">
		<!-- Replace instances -->
		<instance id="organization-organization">
			<web-url>organization</web-url>
			<title-ref>organization</title-ref>
			<title>Corporate Names</title>
		</instance>
		<instance id="organization-committee">
			<web-url>committee</web-url>
			<title-ref>committee</title-ref>
			<title>Committees</title>
		</instance>
	</instances>
	
	<section id="organizationNameAuthorityInformation">
		<!-- Local fields -->
		
		<field id="foundingState" section="cinefiles"/>
		<field id="foundingCity" section="cinefiles"/>

		<repeat id="memberGroupList/memberGroup" section="cinefiles">
			<field id="member" section="cinefiles"/>
			<field id="memberNote" section="cinefiles"/>
			<field id="memberAuthority" section="cinefiles"/>
		</repeat>
		
		<field id="accessCode" section="cinefiles"/>
	</section>
</record>