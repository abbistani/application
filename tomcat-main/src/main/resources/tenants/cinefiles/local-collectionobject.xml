		<field id="hasIllust" section="cinefiles" datatype="boolean"/>
		<field id="hasBiblio" section="cinefiles" datatype="boolean"/>
		
		<field id="docType" section="cinefiles"/>
		
		<repeat id="docSubjects" section="cinefiles">
			<field id="docSubject" section="cinefiles"/>
		</repeat>
		
		<repeat id="nameSubjects" section="cinefiles">
			<field id="nameSubject" section="cinefiles"/>
		</repeat>
		
		<repeat id="filmSubjects" section="cinefiles">
			<field id="filmSubject" section="cinefiles"/>
		</repeat>
		
		<repeat id="docLanguages" section="cinefiles">
			<field id="docLanguage" section="cinefiles"/>
